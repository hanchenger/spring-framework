<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="file=%C line=%L %d{HH:mm:ss.SSS} [%t] %highlight{%-5level} - %msg%n"/>
        </Console>
    </appenders>


    <loggers>
        <root level="debug">
            <appender-ref ref="Console"/>
        </root>
    </loggers>
</configuration>
